<div class='submit-wiki-user'>
  <%= form_tag '/books/wiki_user_info', class: 'wiki-user-info', method: :get do |f| %>
  <label>Please enter your Wiki ID:</label> <br />
  <%= text_field_tag :user_name, '' %>
  <%= hidden_field_tag :book_name, @wiki_book['book_name'] %>
  <%= hidden_field_tag :book_name, @wiki_book['book_id'] %>
  <%= hidden_field_tag :book_name, @wiki_book['library'] %>
  <br />
  <%= submit_tag "Finish", class: "btn btn-success wiki-btn"%>
  <% end %>
</div>